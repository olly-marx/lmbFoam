FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha1 0
    volScalarFieldValue conductivity 187.1
);

// Variable to store the height of the electrolyte layer
h 0.03;

regions
(
    boxToCell
    {
        box (-10 -10 -10) (10 10 $h);
        fieldValues
        (
            volScalarFieldValue alpha1 1
            volScalarFieldValue conductivity 781000
        );
    }
);

