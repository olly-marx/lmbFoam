FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.03";
    object      bodyForce3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    lowerWall
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    upperWall
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


