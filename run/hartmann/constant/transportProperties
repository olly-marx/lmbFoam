{
    transportModel  Newtonian;
    nu              nu [ 0 2 -1 0 0 0 0 ] 1; // OJM40
    rho             rho [ 1 -3 0 0 0 0 0 ] 1; // OJM40;
    conductivity    conductivity [-1 -3 3 0 0 2 0] 1; // OJM40
    mu		    mu [ 1 1 -2 0 0 -2 0 ] 1; // OJM40
}

phase2
{
    transportModel  Newtonian;
    nu              nu [ 0 2 -1 0 0 0 0 ] 1; // OJM40
    rho             rho [ 1 -3 0 0 0 0 0 ] 1; // OJM40;
    conductivity    conductivity [-1 -3 3 0 0 2 0] 1; // OJM40
