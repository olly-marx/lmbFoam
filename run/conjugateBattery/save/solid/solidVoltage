FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      solidVoltage;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    steelInterface
    {
        type            regionCoupling;
        remoteField     Voltage;
        value           uniform 0;
    }
    lithiumInterface
    {
        type            regionCoupling;
        remoteField     Voltage;
        value           uniform 0;
    }
    outsideSurface
    {
        type            zeroGradient;
    }
    topSurface
    {
        type            fixedValue;
        value           uniform 0;
    }
    topLithium
    {
        type            fixedValue;
        value           uniform -0.7;
    }
}

// ************************************************************************* //
