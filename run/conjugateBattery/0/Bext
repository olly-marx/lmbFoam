FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Bext;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform (0 0 -0.0005 );// Earth is .25 to .65 Gauss, we want 10
				       // to 100 times that, so 2.5 to 65 Gauss

boundaryField
{
    liquidToSteelInterface
    {
        type            zeroGradient;
    }
    liquidToLithiumInterface
    {
	type		zeroGradient;
    }
    topSurface
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
