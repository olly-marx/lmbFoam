FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      uniform 0.1;
        value           uniform 0.1;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.1;
        value           uniform 0.1;
    }
    lowerWall
    {
        type            epsilonWallFunction;
        refValue        uniform 0.1;
        value           uniform 0.1;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
    }
    upperWall
    {
        type            epsilonWallFunction;
        refValue        uniform 0.1;
        value           uniform 0.1;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
    }
    frontAndBack
    {
        type            empty;
    }
}


